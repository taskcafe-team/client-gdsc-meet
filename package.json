{
	"name": "gdscmeet",
	"author": "taskcafe team",
	"email": "dangnhatminh@gmail.com",
	"license": "MIT",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "cross-env ENV=development PORT=3000 IO_PORT=3030 webpack serve --mode=development --hot",
		"build": "tsc && webpack --mode=production",
		"build:esm": "tsc && cross-env ESM=true webpack --mode=production",
		"preview": "cross-env PORT=3000 NODE_NO_WARNINGS=1 node ./config/webpack.serve.config.js",
		"prettier": "tsc --noEmit && prettier src/**/*.{t,j}s{,x} --no-error-on-unmatched-pattern --check",
		"lint": "tsc --noEmit && eslint src/**/*.{t,j}s{,x} --no-error-on-unmatched-pattern --ignore-pattern node_modules/",
		"lint:fix": "npm run lint -- --fix",
		"prettier:fix": "npm run prettier -- --write",
		"format": "npm run prettier:fix && npm run lint:fix",
		"prepare": "git config core.autocrlf false && npx husky install",
		"pre-commit": "tsc && lint-staged"
	},
	"browserslist": [
		"> 1%",
		"last 2 versions",
		"not dead"
	],
	"overrides": {
		"isomorphic-fetch@*": "$isomorphic-fetch"
	},
	"devDependencies": {
		"@babel/core": "^7.22.6",
		"@babel/plugin-proposal-class-properties": "^7.18.6",
		"@babel/plugin-syntax-flow": "^7.14.5",
		"@babel/plugin-transform-react-jsx": "^7.14.9",
		"@babel/preset-env": "^7.22.6",
		"@babel/preset-react": "^7.22.5",
		"@babel/preset-typescript": "^7.22.5",
		"@commitlint/config-conventional": "^17.6.6",
		"@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
