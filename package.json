		"@testing-library/dom": "^7.21.4",
		"@types/react": "^18.2.14",
		"@types/styled-components": "^5.1.29",
		"@types/webpack-env": "^1.18.1",
		"@typescript-eslint/eslint-plugin": "^7.0.0-alpha.0",
		"@typescript-eslint/parser": "^6.0.0",
		"autoprefixer": "^10.4.14",
		"babel-loader": "^9.1.2",
		"buffer": "^6.0.3",
		"clean-webpack-plugin": "^4.0.0",
		"commitizen": "^4.3.0",
		"commitlint": "^17.6.6",
		"copy-webpack-plugin": "^11.0.0",
		"core-js": "^3.31.0",
		"cross-env": "^7.0.3",
		"css-loader": "^6.8.1",
		"css-minimizer-webpack-plugin": "^5.0.1",
		"cz-git": "^1.6.1",
		"esbuild-loader": "^4.0.2",
		"eslint": "^8.44.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-prettier": "^8.8.0",
		"eslint-import-resolver-custom-alias": "^1.3.2",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-jsx-a11y": "^6.7.1",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"espree": "^9.6.0",
		"html-webpack-plugin": "^5.5.3",
		"https-browserify": "^1.0.0",
		"husky": "^8.0.3",
		"isomorphic-fetch": "^3.0.0",
		"lint-staged": "^13.2.3",
		"mini-css-extract-plugin": "^2.7.6",
		"nodemon": "^2.0.22",
		"path-browserify": "^1.0.1",
		"postcss": "^8.4.24",
		"postcss-loader": "^7.3.3",
		"postcss-preset-env": "^9.0.0",
		"postcss-simple-vars": "^7.0.1",
		"prettier": "^2.8.8",
		"purgecss-webpack-plugin": "^5.0.0",
		"quicktype-core": "^23.0.49",
		"react-refresh": "^0.14.0",
		"socket.io": "^4.7.1",
		"stream-http": "^3.2.0",
		"swc-loader": "^0.2.3",
		"tailwindcss": "^3.3.5",
		"terser-webpack-plugin": "^5.3.9",
		"tsconfig-paths-webpack-plugin": "^4.1.0",
		"unplugin-auto-import": "^0.16.5",
		"url": "^0.11.3",
		"webpack": "^5.88.1",
		"webpack-cli": "^5.1.4",
		"webpack-dev-server": "^4.15.1"
	},
	"dependencies": {
		"@ant-design/icons": "^5.2.6",
		"@emotion/react": "^11.11.1",
		"@emotion/styled": "^11.11.0",
		"@livekit/components-react": "^1.3.0",
		"@livekit/components-styles": "^1.0.6",
		"@mui/icons-material": "^5.14.15",
		"@mui/joy": "^5.0.0-beta.14",
		"@mui/lab": "^5.0.0-alpha.150",
		"@mui/material": "^5.14.15",
		"@mui/styles": "^5.14.16",
		"@reduxjs/toolkit": "^1.9.7",
		"@testing-library/jest-dom": "^5.14.1",
		"@testing-library/react": "^13.0.0",
		"@testing-library/user-event": "^13.2.1",
		"axios": "^1.6.0",
		"formik": "^2.4.5",
		"framer-motion": "^10.16.4",
		"livekit-client": "^1.14.2",
		"lodash": "^4.17.21",
		"moment": "^2.29.4",
		"next-themes": "^0.2.1",
		"path-to-regexp": "^6.2.1",
		"polished": "^4.2.2",
		"prop-types": "^15.8.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-icons": "^4.11.0",
		"react-redux": "^8.1.3",
		"react-router-dom": "^6.14.1",
		"react-scripts": "5.0.1",
		"react-toastify": "^9.1.3",
		"redux": "^4.2.1",
		"redux-thunk": "^2.4.2",
		"sass": "^1.69.5",
		"styled-components": "^6.0.2",
		"ts-loader": "^9.5.0",
		"typescript": "^5.2.2",
		"web-vitals": "^2.1.0",
		"yup": "^1.3.2"
	},
	"eslintConfig": {
		"extends": [
			"./config/eslint.config.js"
		]
	},
	"prettier": "./config/prettier.config.js",
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": "npm run prettier:fix && npm run lint"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		],
		"rules": {
			"type-enum": [
				2,
				"always",
				[
					"feat",
					"style",
					"test",
					"fix",
					"docs",
					"release",
					"update",
					"perf",
					"chore"
				]
			]
		}
	},
	"config": {
		"commitizen": {
			"path": "cz-git",
			"czConfig": "./config/cz.config.js"
		}
	}
}
